{
  "name": "pc-ui",
  "version": "0.1.0",
  "description": "Personal CRM / Address Book",
  "author": "Jonathan Paul Wood",
  "private": true,
  "main": "public/electron.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test-coverage": "react-scripts test --collect-coverage",
    "lint": "eslint ./src",
    "lint-fix": "eslint ./src --fix",
    "lint-fix-dry-run": "eslint ./src --fix-dry-run",
    "eject": "react-scripts eject",
    "dev": "concurrently -k \"cross-env BROWSER=none npm start\" \"npm:electron\"",
    "electron": "wait-on tcp:3000 && electron ."
  },
  "dependencies": {
    "@popperjs/core": "^2.6.0",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.2.2",
    "axios": "^0.21.1",
    "bootstrap": "^4.5.3",
    "bootstrap-icons": "^1.1.0",
    "classnames": "^2.2.6",
    "concurrently": "^7.6.0",
    "content-disposition": "^0.5.3",
    "cross-env": "^7.0.3",
    "history": "4.10.1",
    "html-react-parser": "^0.14.1",
    "js-cookie": "^2.2.1",
    "js-cookies": "^1.0.4",
    "node-sass": "^4.14.1",
    "prop-types": "^15.7.2",
    "qs": "^6.9.4",
    "react": "^17.0.1",
    "react-beautiful-dnd": "^13.0.0",
    "react-bootstrap": "^1.4.0",
    "react-datepicker": "^3.3.0",
    "react-dom": "^17.0.1",
    "react-popper": "^2.2.4",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0",
    "react-select": "^3.1.0",
    "react-toastify": "^6.1.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "remarkable": "^2.0.1",
    "styled-components": "^5.2.1",
    "uuid": "^8.3.1",
    "wait-on": "^6.0.1",
    "web-vitals": "^2.1.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^22.0.0",
    "electron-is-dev": "^2.0.0"
  }
}
